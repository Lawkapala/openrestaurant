{
  "name": "drupal/openrestaurant",
  "description": "Everything you need to build your Restaurant website.",
  "type": "drupal-profile",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {},
  "config": {
    "bin-dir": "bin/"
  },
  "repositories": [
    {
      "type": "composer",
      "url":  "https://packages.drupal.org/8"
    }
  ],
  "scripts": {},
  "extra": {
    "installer-paths": {
      "docroot/core": [
        "drupal/core"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ]
    }
  },
  "require": {
    "drupal/core": "^8.1.0",
    "drupal/address": "8.1.0-beta2",
    "drupal/admin_toolbar": "8.1.14",
    "drupal/config_update": "8.1.1",
    "drupal/ctools": "8.3.0-alpha25",
    "drupal/composer_manager": "8.1.0-rc1",
    "drupal/features": "8.3.0-beta3",
    "drupal/pathauto": "1.0-alpha3",
    "drupal/token": "8.1.0-alpha2",
    "drupal/radix": "3.0.x-dev"
  }
}
